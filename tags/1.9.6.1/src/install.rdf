<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.mozilla.org/2004/em-rdf#"
	 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <rdf:Description about="urn:mozilla:install-manifest">
    <id>jsonovich@lackoftalent.org</id>
    <name>JSONovich</name>
    <version>1.9.6.1</version>
    <type>2</type>
    <bootstrap>true</bootstrap>
    <optionsType>2</optionsType>

    <description>Pretty-prints JSON content in the browser for easy, unobtrusive viewing.</description>
    <homepageURL>http://lackoftalent.org/michael/blog/json-in-firefox/</homepageURL>
    <creator>Michael J. Giarlo</creator>
    <developer>Will Elwood</developer>

    <!-- Firefox -->
    <targetApplication>
      <rdf:Description>
        <id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</id>
        <minVersion>3.5</minVersion>
        <maxVersion>17.*</maxVersion>
      </rdf:Description>
    </targetApplication>

    <!-- Seamonkey -->
    <targetApplication>
      <rdf:Description>
        <id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</id>
        <minVersion>2.0</minVersion>
        <maxVersion>2.14.*</maxVersion>
      </rdf:Description>
    </targetApplication>

    <!-- Fennec -->
    <targetApplication>
      <rdf:Description>
        <id>{a23983c0-fd0e-11dc-95ff-0800200c9a66}</id>
        <minVersion>1.1</minVersion>
        <maxVersion>12.*</maxVersion>
      </rdf:Description>
    </targetApplication>

    <!-- Fennec NativeUI (speculatively added in case someone is able to test before me :) ~WE ) -->
    <targetApplication>
      <rdf:Description>
        <id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</id>
        <minVersion>10.0a1</minVersion>
        <maxVersion>17.*</maxVersion>
      </rdf:Description>
    </targetApplication>
  </rdf:Description>
</rdf:RDF>
