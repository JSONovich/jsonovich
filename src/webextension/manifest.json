{
    "manifest_version": 2,
    "name": "JSONovich",
    "version": "1.99.1",
    "description": "Pretty-prints JSON content in the browser for easy, unobtrusive viewing.",
    "homepage_url": "https://github.com/mjgiarlo/jsonovich",
    "author": "Michael J. Giarlo",
    "developer": {
        "name": "William Elwood"
    },
    "applications": {
        "gecko": {
            "id": "jsonovich@lackoftalent.org",
            "strict_min_version": "52.0a1"
        }
    },
    "background": {
        "scripts": [
            "migrate.js"
        ]
    },
    "permissions": [
        "storage"
    ]
}
