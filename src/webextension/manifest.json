{
    "manifest_version": 2,
    "name": "JSONovich",
    "version": "1.99.4",
    "description": "Pretty-prints JSON content in the browser for easy, unobtrusive viewing.",
    "homepage_url": "https://github.com/mjgiarlo/jsonovich",
    "author": "Michael J. Giarlo",
    "developer": {
        "name": "William Elwood"
    },
    "applications": {
        "gecko": {
            "id": "jsonovich@lackoftalent.org",
            "strict_min_version": "52.0a1"
        }
    },
    "background": {
        "scripts": [
            "common.js",
            "migrate.js",
            "background.js"
        ]
    },
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "resources/*"
    ]
}
