{
    "manifest_version": 2,
    "default_locale": "en",
    "name": "JSONovich",
    "version": "1.99.6alpha1",
    "description": "__MSG_extensionDescription__",
    "homepage_url": "https://github.com/mjgiarlo/jsonovich",
    "author": "Michael J. Giarlo",
    "developer": {
        "name": "William Elwood"
    },
    "applications": {
        "gecko": {
            "id": "jsonovich@lackoftalent.org",
            "strict_min_version": "52.0a1"
        }
    },
    "background": {
        "scripts": [
            "common.js",
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options/options.html",
        "browser_style": true
    },
    "icons": {
        "32": "icon32.png",
        "64": "icon64.png"
    },
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "webNavigation",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "resources/*"
    ]
}